<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.airsense.view.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.airsense.view.dashboard.DashBoardContainer"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard_container" />
    <fragment
        android:id="@+id/navigation_graph"
        android:name="com.example.airsense.view.graph.GraphFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_chart" />
    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.airsense.view.account.AccountFragment"
        android:label="@string/title_account"
        tools:layout="@layout/fragment_account"/>
</navigation>